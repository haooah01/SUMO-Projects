<?xml version="1.0" encoding="UTF-8"?>
<edges>
    <!-- left-to-right edges -->
    <edge id="1to2" from="1" to="2" />
    <edge id="out" from="2" to="3" />

    <!-- right-to-left (reverse) edges so vehicles can travel from right to left -->
    <edge id="3to2" from="3" to="2" />
    <edge id="2to1" from="2" to="1" />
</edges>
